/* components/nav-bar/nav-bar.wxss */
.container {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 99;
}
.nav-bar {
    position: relative;
    width: 100%;
    text-align: center;
    line-height: 48px;
    font-size: 34rpx;
    padding: 0 28rpx;
    box-sizing: border-box;
}
.backBtn {
    float: left;
    width: 34rpx;
    height: 34rpx;
}
.backBtn image {
    width: 100%;
    height: 100%;
}